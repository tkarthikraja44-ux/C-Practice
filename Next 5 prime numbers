#include<iostream>
using namespace std;
int main()
{
    int n=0;
    int count = 0;
    cout <<"Enter the number:";
    cin >> n;
    while(count < 5)
    {
        n++;
        int i;
        for(i =2;i<n;i++)
        {
            if(n%i==0)
            {
                break;
            }
        }
        if(i*i>n)
        {
            count++;
            cout << n <<" ";
        }
    }
    
}
