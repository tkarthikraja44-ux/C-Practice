#include<iostream>
using namespace std;
int main()
{
    int num=0;
    cin >> num;
    if(num<=0||num>40)
    {
        cout << "INVALID";
    }
    if(num==40)
    {
        cout <<"XL";
        num -=40;
    }
    while(num>=10)
    {
        cout <<"X";
        num -= 10;
    }
    if(num==9)
    {
        cout <<"IX";
        num -=9;
    }
    if(num>=5)
    {
        cout <<"V";
        num-=5;
    }
    if(num==4)
    {
        cout <<"IV";
        num-=4;
    }
    while(num>=1)
    {
        cout <<"I";
        num-=1;
    }
}
