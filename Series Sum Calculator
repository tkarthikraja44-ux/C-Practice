#include<iostream>
using namespace std;
int main()
{
    int s=0;
    int t=0;
    cin >> s;
    cin >> t;
    int sum=0;
    int f=0;
    for(int i = 0;i<t;i++)
    {
         sum = (sum*10) + s;
         f++;
         if(f<t)
         {
             cout << sum << "+";
         }
    }
    cout << sum;
}
