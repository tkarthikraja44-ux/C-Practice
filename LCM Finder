#include<iostream>
using namespace std;
int main()
{
    int num1=0,num2=0;
    cin >> num1 >> num2;
    int a=0,b=0;
    int lcm = 0,hcf=0,rem = 0;
    a = num1;
    b = num2;
    while(b!=0)
    {
        rem = a%b;
        a = b;
        b =rem;
    }
    hcf =a;
    lcm = (num1*num2) / hcf;
    cout << lcm;
}
