#include<iostream>
using namespace std;
int main()
{
    int num=0;
    cin >> num;

    while(true) 
    {
        num++;
        int rev=0;
        int temp = num;

        while(temp>0)  
        {
            rev = rev*10 + temp%10;
            temp /= 10;
        }

        if(num == rev)  
        {
            cout << num;
            break;   
        }
    }
}
