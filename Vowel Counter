#include<iostream>
#include<string>
using namespace std;
int main()
{
    string word;
    cin >> word;
    int count =0;
    for(int i =0;i<word.length();i++)
    {
        char ch = tolower(word[i]);
        if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u')
        {
            count++;
        }
    }
    cout << count;
}
