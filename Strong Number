#include<iostream>
using namespace std;
int  main()
{
    int num = 0;
    cin >>num;
    int og=num;
    int sum=0;
    while(num>0)
    {
        int f=1;
        int digit =0;
        digit = num%10;
        for(int i=1;i<=digit;i++)
        {
            f*=i;
        }
        sum+=f;
        num/=10;
    }
    if(sum==og)
    {
        cout <<"Strong number";
    }
    else
    {
        cout <<"not";
    }
}
